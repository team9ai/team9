{
  "name": "@team9/gateway",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "node --loader @swc-node/register/esm --watch src/main.ts",
    "start:debug": "node --inspect --loader @swc-node/register/esm src/main.ts",
    "start:prod": "node dist/main.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.cjs",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.cjs --watch",
    "test:cov": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.cjs --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "optionalDependencies": {
    "@team9/enterprise-sso": "workspace:*",
    "@team9/enterprise-audit": "workspace:*",
    "@team9/enterprise-analytics": "workspace:*",
    "@team9/enterprise-license": "workspace:*"
  },
  "dependencies": {
    "@team9/ai-client": "workspace:*",
    "@team9/auth": "workspace:*",
    "@team9/database": "workspace:*",
    "@team9/email": "workspace:*",
    "@team9/rabbitmq": "workspace:*",
    "@team9/redis": "workspace:*",
    "@team9/shared": "workspace:*",
    "@team9/storage": "workspace:*",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.10",
    "@nestjs/websockets": "^11.1.10",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/microservices": "^11.0.1",
    "@grpc/grpc-js": "^1.12.0",
    "@grpc/proto-loader": "^0.7.13",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "@socket.io/redis-adapter": "^8.3.0",
    "transliteration": "^2.6.0",
    "uuid": "^13.0.0"

{
  "name": "@team9/gateway",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "node --loader @swc-node/register/esm --watch src/main.ts",
    "start:debug": "node --inspect --loader @swc-node/register/esm src/main.ts",
    "start:prod": "node dist/main.js",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.cjs",
    "test:watch": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.cjs --watch",
    "test:cov": "NODE_OPTIONS='--experimental-vm-modules' jest --config jest.config.cjs --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@team9/ai-client": "workspace:*",
    "@team9/auth": "workspace:*",
    "@team9/database": "workspace:*",
    "@team9/rabbitmq": "workspace:*",
    "@team9/redis": "workspace:*",
    "@team9/shared": "workspace:*"
  }
}

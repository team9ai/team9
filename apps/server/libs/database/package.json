{
  "name": "@team9/database",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./schemas": "./src/schemas/index.ts",
    "./config": "./src/config.service.ts"
  },
  "scripts": {
    "db:generate": "dotenv -e ../../.env -- drizzle-kit generate",
    "db:migrate": "dotenv -e ../../.env -- drizzle-kit migrate",
    "db:push": "dotenv -e ../../.env -- drizzle-kit push",
    "db:studio": "dotenv -e ../../.env -- drizzle-kit studio"
  },
  "dependencies": {
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.8"
  }
}
